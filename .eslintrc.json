{
    "env": { 
        "es6": true,
        "browser": true
    },
    "extends": ["eslint:recommanded", "plugin:react/recommanded"],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": ["react"],
    "rules": {
        "semi": "error",
        "no-console": 0,
        "react/jsx-uses-vars": "error"
    }
}